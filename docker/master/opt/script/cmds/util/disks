CTYPE( )
{
 echo text/plain
}

STRATEGY( )
{
  echo async
}

RUN( )
{

 CBDATA=$(
 for u in $(ls /dev/disk/by-uuid/)
 do 
	echo \{\"$u\":\{\"size\":$(blockdev --getsize64 /dev/disk/by-uuid/$u),\"block\":$(blockdev --getpbsz /dev/disk/by-uuid/$u),\"realdev\":\"$(readlink /dev/disk/by-uuid/$u | cut -d '/' -f 3-)\"\}\}
 done | jq -c .)
 echo $CBDATA
 CALLBACK $CBDATA
}
